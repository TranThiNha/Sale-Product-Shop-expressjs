                indexs.push(index);
                alert("Delete data in row (" + (index + 1) + ") ?");
            });


            
            if (indexs.length == 0){
              alert("No product selected!")
            }else{
                 
                  window.location = "/table.html?action="+"delete"+"&indexs="+indexs;
              
            }
                
        });

        

        $(".view-detail").each(function(index) {
          $(this).on("click", function() {
