<script type="text/javascript">
function deleteImage(imgId, delId){

id_tag =  document.getElementById(imgId).src
    position = $.inArray(id_tag, images);
if ( ~position ) colors.splice(position, 1);
  document.getElementById(delId).style.display = 'none';
  document.getElementById(imgId).src = "/images/placeholder.png" 
}
function deleteColor(colorId, delId){
    id_tag = document.getElementById(colorId).style.background
    position = $.inArray(id_tag, colors);
if ( ~position ) colors.splice(position, 1);
  document.getElementById(colorId).style.display = 'none';
  document.getElementById(delId).style.display = 'none';
}

</script>

<script type="text/javascript">
  function chooseColors(thelist) {
    var idx = thelist.selectedIndex;
    var content = thelist.options[idx].innerHTML;

    for (i = 1 ; i < 17 ; i++){

      if (!document.getElementById("divcolor"+ i.toString()).style.background){
        document.getElementById("divcolor"+ i.toString()).style.background = content;
        colors = colors + content + ","
         document.getElementById('color').value = colors
        document.getElementById("divcolor"+ i.toString()).style.display = 'block'
        document.getElementById("del-color-"+ i.toString()).style.display = 'block'
        break;
      }
          
    }

}
    function chooseCategory(thelist) {
      var idx = thelist.selectedIndex;
      var content = thelist.options[idx].innerHTML;
