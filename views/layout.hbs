<script type="text/javascript">
	
	function chooseNumberPerPage(thelist){

		var idx = thelist.selectedIndex;
    	var content = thelist.options[idx].innerHTML;


    	window.location = '/index.html?category={{category}}&number_per_page='+content;
	}

</script>

<script type="text/javascript">
	function setGender(thelist){
		var idx = thelist.selectedIndex;
    	var content = thelist.options[idx].innerHTML;
    	document.getElementById('gender').value = content;
	}
</script>


<script type="text/javascript">
	
function orderProduct(){

	var number_list = [];

	var table = document.getElementById('item_tbl');
        for (var r = 0, n = table.rows.length; r < n; r++) {
            var m = table.rows[r].cells.length;
            number_list.push(table.rows[r].cells[m-1].children[0].value);
           // alert(table.rows[r].cells[m-1].children[0].value);//getElementsByTagName("input")[0].value);
            
        }

	window.location = '/order_product.html?number_list='+number_list;
}

</script>

<script type="text/javascript">
	
	function order_success(){
		window.location.replace('/card.html');
	}
</script>

<script type="text/javascript">
	
	
	function profile_click(){

		if (document.getElementById('drop_menu_profile').style.display == 'none'){
			document.getElementById('drop_menu_profile').style.display = 'block';
		}else {
			document.getElementById('drop_menu_profile').style.display = 'none';
		}
		
	}

</script>

<script type="text/javascript">
	

	function showOrder()
    {
     
      document.getElementById("Profile").style.display = "none"; 
      document.getElementById("Order").style.display = "block"; 
    }

    function showProfile()
    {
     
      document.getElementById("Order").style.display = "none"; 
      document.getElementById("Profile").style.display = "block"; 
    }
</script>

<script type="text/javascript">
	function  readURL(input,imgId) {
        if (input.files && input.files[0]) {
            var reader = new FileReader();
            var src = "jhbhjb"  
            reader.onload = function (e) {

              document.getElementById(imgId).src =  e.target.result
                    
            };          
                 
            reader.readAsDataURL(input.files[0] ); 
            
        }
    }
</script>

